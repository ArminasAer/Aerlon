{% extends "layouts/app-layout.ehtml" %}

{% block title %}
{% endblock %}

{% block content %}
{% for meta in metaList %}
<div>
<p>{{ meta.Id }}</p>
<p>{{ meta.Title }}</p>
<p>{{ meta.Slug }}</p>
<p>{{ meta.Date }}</p>
<p>{{ meta.Series }}</p>
{% for c in meta.Categories %}
<span>{{ c }}-</span>
{% endfor %}
</div>
<br/>
{% endfor %}
{% endblock %}
