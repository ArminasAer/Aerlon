<h2 class="posts-label">Featured Posts</h2>
{% for meta in metaList %}
    {% if meta.Featured %}
        {% include "components/post_card.ehtml" %}
    {% endif %}
{% endfor %}
<h2 class="posts-label">Latest Posts</h2>
{% for meta in metaList %}
    {% if !meta.Featured %}
        {% include "components/post_card.ehtml" %}
    {% endif %}
{% endfor %}